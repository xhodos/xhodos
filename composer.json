{
  "name": "xhodos/xhodos",
  "type": "project",
  "description": "The skeleton application for the xhodos PHP web application framework.",
  "version": "1.1.0",
  "keywords": [
    "xhodos",
    "framework"
  ],
  "license": "MIT",
  "require-dev": {
    "php": "^8.4",
    "xhodos/framework": "^v1.2.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Hodos\\": "vendor/xhodos/framework/",
      "setasign\\Fpdi\\": "vendor/setasign/fpdi/src/",
      "setasign\\FpdiPdfParser\\PdfParser\\": "vendor/setasign/fpdi/src/PdfParser/"
    }
  },
  "require": {
    "setasign/fpdi": "^2.6",
    "setasign/fpdf": "^1.8",
    "ext-mbstring": "*",
    "ext-mysqli": "*"
  }
}
